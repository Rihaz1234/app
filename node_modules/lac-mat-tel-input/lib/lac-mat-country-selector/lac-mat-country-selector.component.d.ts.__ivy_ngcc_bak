import { OnInit, EventEmitter, OnDestroy } from '@angular/core';
import { Country } from '../model/country.model';
import { CountryCode } from '../data/country-code';
import { LacMatTelInputComponent } from '../lac-mat-tel-input.component';
import { MatInput } from '@angular/material/input';
export declare class LacMatCountrySelectorComponent implements OnInit, OnDestroy {
    private countryCodeData;
    selectorFor: LacMatTelInputComponent;
    preferredCountries: Array<string>;
    onlyCountries: Array<string>;
    showDialCode?: boolean;
    disableSearch?: boolean;
    searchPlaceholder?: string;
    change: EventEmitter<Country>;
    selectedCountry: Country;
    allCountries: Array<Country>;
    preferredCountriesInDropDown: Array<Country>;
    get disabled(): boolean;
    searchQuery: string;
    searchInput: MatInput;
    private countrySubscription;
    constructor(countryCodeData: CountryCode);
    protected fetchCountryData(): void;
    ngOnInit(): void;
    ngOnDestroy(): void;
    selectCountry(country: Country): void;
    private onCountryUpdateFromInput;
    onOpenMenuClick(): void;
    onMenuClosed(): void;
}
